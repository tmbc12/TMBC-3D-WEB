<script src="https://moment.github.io/luxon/global/luxon.js"></script>
<script>
var DateTime = luxon.DateTime;
const locations = document.querySelectorAll("section.times div");
const updateTimes = function () {
  locations.forEach(location => {
    const outputs = location.querySelectorAll(".output")
    const timezone = location.getAttribute("data-timezone")
    const now = luxon.DateTime.now().setZone(timezone)
    outputs.forEach(output => {
      output.innerHTML = now.toFormat("HH:mm:ss")
    })
    const hour = parseInt(now.toFormat("H"))
    if (hour >= 9 && hour < 18){
    	location.classList.add("open")
    }
  })
}
updateTimes()
</script>

<style>
section.times div.time-array.open {opacity:1;}
</style>

<script>
setInterval(function () {
	updateTimes()
}, 1000)
</script>

<script>
$('.timeout').click(function (e) {
    e.preventDefault();
    var goTo = this.getAttribute("href");

    setTimeout(function(){
         window.location = goTo;
    },1500);       
});
</script>

<script>
$("#wf-form-Brief").submit(function(e){
   e.preventDefault();
   var href = $(this).attr("action");
   $.ajax({
      type: "POST",
      dataType: "json",
      url: href,
      data: $(this).serialize(),
      success: function(response){
         if(response.status === "success"){
           $('#wf-form-Brief')[0].reset();
           $('.w--redirected-checked').removeClass('w--redirected-checked');
         }else{
            console.log("An error occured: " + response.message);
         }
      }
   });
});
</script>

<script src="https://unpkg.com/flickity@2/dist/flickity.pkgd.min.js"></script>
<script>
let mainCarousel1 = ".tricks-slider-1";
let mainSlides1 = ".tricks-slider_slide-1";
let mainCarousel2 = ".tricks-slider-2";
let mainSlides2 = ".tricks-slider_slide-2";
let mainCarousel3 = ".tricks-slider-3";
let mainSlides3 = ".tricks-slider_slide-3";

let parallaxPercentage = 49;

let flkty1 = new Flickity(mainCarousel1, {
  contain: true,
  freeScroll: true,
  percentPosition: true,
  pageDots: false,
  cellSelector: mainSlides1,
  cellAlign: "left",
  resize: true,
  selectedAttraction: 0.01,
  dragThreshold: 1,
  freeScrollFriction: 0.05
});
let flkty2 = new Flickity(mainCarousel2, {
  contain: true,
  freeScroll: true,
  percentPosition: true,
  pageDots: false,
  cellSelector: mainSlides2,
  cellAlign: "left",
  resize: true,
  selectedAttraction: 0.01,
  dragThreshold: 1,
  freeScrollFriction: 0.05
});
let flkty3 = new Flickity(mainCarousel3, {
  contain: true,
  freeScroll: true,
  percentPosition: true,
  pageDots: false,
  cellSelector: mainSlides3,
  cellAlign: "left",
  resize: true,
  selectedAttraction: 0.01,
  dragThreshold: 1,
  freeScrollFriction: 0.05
});

flkty1.on("scroll", function (progress) {
  setImagePositions(mainCarousel1, mainSlides1);
  $(".progress_fill-1").css("width", `${progress * 100}%`);
});
flkty2.on("scroll", function (progress) {
  setImagePositions(mainCarousel2, mainSlides2);
  $(".progress_fill-2").css("width", `${progress * 100}%`);
});
flkty3.on("scroll", function (progress) {
  setImagePositions(mainCarousel3, mainSlides3);
  $(".progress_fill-3").css("width", `${progress * 100}%`);
});

function setImagePositions(main, slides) {
  $(slides).each(function (index) {
    let targetElement = $(this);
    let elementOffset =
      targetElement.offset().left +
      targetElement.width() -
      $(main).offset().left;
    let parentWidth = $(main).width() + targetElement.width();
    let myProgress = elementOffset / parentWidth;
    let slideProgress = parallaxPercentage * myProgress;
    if (slideProgress > parallaxPercentage) {
      slideProgress = parallaxPercentage;
    } else if (slideProgress < 0) {
      slideProgress = 0;
    }
    targetElement
      .find(".image")
      .css("transform", `translateX(-${slideProgress}%)`);
  });
}

setImagePositions(mainCarousel1, mainSlides1);
setImagePositions(mainCarousel2, mainSlides2);
setImagePositions(mainCarousel3, mainSlides3);
</script>



<script src="https://cdn.jsdelivr.net/gh/studio-freight/lenis@0.2.28/bundled/lenis.js"></script>
<script>
const lenis = new Lenis({
  duration: 2.4,
  easing: (t) => Math.min(1, 1.001 - Math.pow(2, -10 * t)), // https://www.desmos.com/calculator/brs54l4xou
  direction: 'vertical', // vertical, horizontal
  gestureDirection: 'vertical', // vertical, horizontal, both
  smooth: true,
  mouseMultiplier: 1,
  smoothTouch: false,
  touchMultiplier: 0.9,
  infinite: false,
})


function raf(time) {
  lenis.raf(time)
  requestAnimationFrame(raf)
}

requestAnimationFrame(raf)
</script>



<script>
  const blockScroll = () => {
    if (typeof document === 'undefined') return;
    const html = document.documentElement;
    const {body} = document;
    if (!body || !body.style) return;
    const scrollBarWidth = window.innerWidth - html.clientWidth;
    const bodyPaddingRight =
            parseInt(
                    window.getComputedStyle(body).getPropertyValue('padding-right'),
            ) || 0;
    html.style.position = 'relative';
    body.style.position = 'relative';
    html.style.overflow = 'hidden';
    body.style.overflow = 'hidden';
    body.style.paddingRight = `${bodyPaddingRight + scrollBarWidth}px`;
    lenis && lenis.stop();
  };
  const allowScroll = () => {
    if (typeof document === 'undefined') return;
    const html = document.documentElement;
    const {body} = document;
    if (!body || !body.style) return;
    html.style.position = '';
    html.style.overflow = '';
    body.style.position = '';
    body.style.overflow = '';
    body.style.paddingRight = '';
    lenis && lenis.start();
  };

  const listBlockScroll = document.querySelectorAll("[data-button-scroll-block]");
  listBlockScroll && listBlockScroll.forEach(button => {
    button && button.addEventListener('click', (e) => blockScroll());
  });
  const listAllowScroll = document.querySelectorAll("[data-button-scroll-allow]");
  listAllowScroll && listAllowScroll.forEach(button => {
    button && button.addEventListener('click', (e) => allowScroll());
  });

</script>

<script>
  $('.js-disabled_submit') && $('.js-disabled_submit').prop("disabled");
 
  let requiredFields = {};
  
  $("#wf-form-Brief .form_inputs_list_2col .input-required").each(function() {
   requiredFields = {
      ...requiredFields,
      [$(this).attr('name')]: $(this).val().trim() !== '',
    };
    
    $(this).on('input', function() {
      const isCorrect = $(this).attr('name') === 'E-mail' 
      ? /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,}$/i.test($(this).val().trim())
      : $(this).val().trim() !== '';
      requiredFields = {
        ...requiredFields,
        [$(this).attr('name')]: isCorrect,
      };
      
      if (isCorrect) {
       $(this).css('border-color', '#000 #000 rgba(0, 0, 0, 0.1)');
      } else {
       $(this).css('border-color', '#C61616');
      }
      
      if (Object.values(requiredFields).every(item => item)) {
       $('.js-disabled_submit').css('display', 'none');
        $('.js-active_submit').css('display', 'inline-block');
        
      } else {
       $('.js-disabled_submit').css('display', 'inline-block');
        $('.js-active_submit').css('display', 'none');
      }
    });
  });
</script>

<script>
  let fitObjects = Array.from($('.home_work_double'));
  
  function filterFitObjectsByClass(filterParam) {
  	let filtered = fitObjects.filter(item => Array.from(item.classList).includes(filterParam));
    $('.projects_cases').empty().append(filtered);
  };
  
  $('.filter_type').each(function(){
    $(this).on('click', function() {
      	filterFitObjectsByClass($(this).attr('data-filter-type'));
      	$('.cases_filters .subtitle').text($(this).children().first().text());
      	$('.cases_count .subtitle').text($(this).children().last().text());
      	$('.cases_filters_close').click();
    });
  });
</script>

<script>
const letsTalkSteps = [
  {titleClass: 'brief_menu_step_02', formClass: 'brief_type'},
  {titleClass: 'brief_menu_step_03', formClass: 'brief_budget'},
  {titleClass: 'brief_menu_step_04', formClass: 'brief_from'},
];

letsTalkSteps.forEach(step => {
  $(`.${step.titleClass}`).on('mouseover', function () {
    if (Array.from($(`.${step.formClass} .buttons_list_brief input`)).every(item => !($(item).is(':checked')) )) {
      $(this).css('pointer-events', 'none');
    }
  });
  
  $(`.${step.formClass} .buttons_list_brief input`).each(function() {
    $(this).on('click', function() {
      $(`.${step.titleClass}`).css('pointer-events', 'auto');
    })
  });
});
</script>

