if ($('.autoplay-video video').length > 0) {
   $('.autoplay-video video').trigger('play');  
}
</script>

<script>
  $("#wf-form-ensure-form").submit(function(e){
     e.preventDefault();
     var href = $(this).attr("action");
     $.ajax({
        type: "POST",
        dataType: "json",
        url: href,
        data: $(this).serialize(),
        success: function(response){
           if(response.status === "success"){
             $('#wf-form-ensure-form')[0].reset();
           }else{
              console.log("An error occured: " + response.message);
           }
        }
     });
  });
  
  $(document).ready(function() {
  var $arrowButton = $('.js-arrow-indicator');
  
  $(window).scroll(function() {
    var scrollPercentage = ($(window).scrollTop() / ($(document).height() - $(window).height())) * 100;
    
    if (scrollPercentage > 50) {
      $('.indicator-wrap__arrow').css('transform', 'rotate(180deg)');
    } else {
      $('.indicator-wrap__arrow').css('transform', 'rotate(0deg)');
    }
  });
  
  $('.js-arrow-indicator').on('click', function() {
    var scrollPercentage = (window.scrollY / (document.documentElement.scrollHeight - window.innerHeight)) * 100;

    if (scrollPercentage < 50) {
      window.scrollTo({ top: document.documentElement.scrollHeight, behavior: 'smooth', duration: 1000 });
    } else {
      window.scrollTo({ top: 0, behavior: 'smooth', duration: 1000 });
    }
  });
});

$('.js-navbutton-theme').on('click', function() {
  $('.body').toggleClass('body--light');
});
  
$('.js-navbutton-mob-theme').on('click', function() {
  $('.body').toggleClass('body--light');
});
  
  $('.blog__cat-link').on('click', function(e) {
  let catText = $(this).text();
  let urlTo = `/blog?filter-by=${catText}`
  
  window.location.href = urlTo;
})
    
  
document.querySelectorAll('a[href^="#"]').forEach(anchor => {
    anchor.addEventListener('click', function (e) {
        e.preventDefault();

        document.querySelector(this.getAttribute('href')).scrollIntoView({
            behavior: 'smooth'
        });
    });
});

  /* If browser back button was used, flush cache */
(function () {
	window.onpageshow = function(event) {
		if (event.persisted) {
			window.location.reload();
		}
	};
})();
  

  
  $('.blogs__show-more').on('click', function() {
  let activeCategory = $('.blogs__categories-item.fltr-active');
    
    setTimeout(function() {
      activeCategory[0].click();
    }, 300);
    setTimeout(function() {
      scrollToTop('blogs');
    }, 750);
    setTimeout(function() {
      activeCategory[0].click();
    }, 1000);
  
});

function scrollToTop(sectionId) {
  var targetSection = $('#' + sectionId);
  if (targetSection.length > 0) {
    $('html, body').animate({
      scrollTop: targetSection.offset().top
    }, 'slow');
  }
}
</script>

<script>
var currentCloseMenu = document.querySelector('.navbar');

var observerMenu = new MutationObserver(function(mutations) {
    mutations.forEach(function(mutation) {
      if (window.innerWidth < 768) {
        if (mutation.attributeName === 'style') {
            var currentDisplayStyle = currentCloseMenu.style.display;

            if (currentDisplayStyle === 'none') {
                $('html, body').animate({ scrollTop: 0 }, 'slow');
                $('body').css('overflow-y', 'hidden');
                $('body').css('height', '100vh');
            } else {
                $('body').css('overflow-y', 'auto');
                $('body').css('height', 'auto');
            }
        }
      }
    });
});

var config = { attributes: true };

observerMenu.observe(currentCloseMenu, config);


</script>


